{
  "manifest_version": 2,

  "name": "Twitter Social Toolkit",
  "description": "This extension is a twitter social toolkit",
  "version": "1.0",

  "background": {
   "scripts": ["background.js"],
   "persistant" : false
 },


  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "64":"icon.png",
    "32":"icon.png",
    "16":"icon.png",
    "128":"icon.png"
  },
"permissions":[
  "contextMenus"
]

}
